<form method="post" action="{{ path('admin_delete', {'id': user.id}) }}" onsubmit="return confirm('Vous voulez vraiment supprimer cet Utilisateur?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.id) }}">
    <button class="btn" style="background-color:brown;color:white;">Supprimer</button>
</form>
